<AnchorPane fx:id="PANE_2" prefHeight="400.0" prefWidth="400.0" style="-fx-background-color: #333645;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="extra_class_bookingsystem.Controllers.DATE_TIME_SELECT" onKeyPressed="#available_room_show_keyEvent">
   <children>
      <DatePicker fx:id="DATE_PICKER" layoutX="105.0" layoutY="125.0" prefHeight="31.0" prefWidth="210.0" />
      <Label fx:id="DATE_LABEL"  contentDisplay="CENTER" layoutX="117.0" layoutY="62.0" prefHeight="23.0" prefWidth="194.0" text="SELECT DATE" textFill="WHITE">
         <font>
            <Font name="Cooper Black" size="19.0" />
         </font>
      </Label>
      <Label fx:id="TIME_LABEL" contentDisplay="CENTER" layoutX="109.0" layoutY="207.0" prefHeight="23.0" prefWidth="210.0" text="SELECT TIME SLOT" textFill="WHITE">
         <font>
            <Font name="Cooper Black" size="19.0" />
         </font>
      </Label>
      <ComboBox fx:id="TIME_SLOT_BOX" layoutX="105.0" promptText="select time slot" layoutY="259.0" prefHeight="31.0" prefWidth="218.0" />
      <Button fx:id="available_room_button" contentDisplay="BOTTOM" layoutX="170.0" layoutY="328.0" mnemonicParsing="false" onMousePressed="#available_room_show" prefHeight="45.0" prefWidth="93.0" style="-fx-background-color: #0079D7;" text="OK" textFill="WHITE"  textOverrun="WORD_ELLIPSIS">
